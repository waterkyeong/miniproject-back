package pnu.edu.service;

import org.springframework.stereotype.Service;

import lombok.RequiredArgsConstructor;
import pnu.edu.domain.FreeBoard;
import pnu.edu.repository.FreeBoardImgsRepository;

@Service
@RequiredArgsConstructor
public class FreeBoardImgsService {

	private final FreeBoardImgsRepository fbRepo;
	
	public int getfbImg (FreeBoard freeboard) {
		 return fbRepo.findAllById(freeboard.getFreebimgid());
	}
}
